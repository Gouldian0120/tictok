{
    "name": "bpocallaghan/titan",
    "description": "Core framework for laravel projects (Laravel Admin Starter).",
    "keywords": [
        "laravel",
        "titan",
        "traits",
        "models",
        "helpers",
        "admin",
        "cms",
        "pages",
        "photos",
        "page builder",
        "controllers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ben-Piet O'Callaghan",
            "email": "bpocallaghan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "google/recaptcha": "^1.2",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "bpocallaghan/alert": "^1.1",
        "bpocallaghan/notify": "^2.1",
        "bpocallaghan/sluggable": "^1.2",
        "spatie/laravel-analytics": "^3.1",
        "bpocallaghan/impersonate": "^1.0",
        "yajra/laravel-datatables-oracle": "^8.0"
    },
    "require-dev": {
        "bpocallaghan/generators": "^5.0",
        "barryvdh/laravel-ide-helper": "^2.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Bpocallaghan\\Titan\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bpocallaghan\\Titan\\TitanServiceProvider"
            ]
        }
    }
}
