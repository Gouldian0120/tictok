{
    "name": "bpocallaghan/titan",
    "description": "Core framework for laravel projects (Laravel Admin Starter).",
    "keywords": [
        "laravel",
        "titan",
        "traits",
        "models",
        "helpers",
        "admin",
        "cms",
        "pages",
        "photos",
        "page builder",
        "controllers"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ben-Piet O'Callaghan",
            "email": "bpocallaghan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/view_helpers.php",
            "app/Helpers/form_helpers.php",
            "app/Helpers/admin_helpers.php",
            "app/Helpers/upload_helpers.php"
        ],
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Bpocallaghan\\Titan\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bpocallaghan\\Titan\\TitanServiceProvider"
            ]
        }
    }
}
